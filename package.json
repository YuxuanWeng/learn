{
  "name": "oms-fe",
  "version": "2.11.4",
  "localServer": {
    "version": "2.12.4",
    "shortHash": ""
  },
  "main": "main/index.js",
  "repository": {
    "type": "git",
    "url": "https://git.zoople.cn/shihe/project_group/bdm/bds/bds-fe.git"
  },
  "scripts": {
    "prepare": "husky install && tsx scripts/prepare/local-server.ts",
    "dev": "pnpm scripts:build-electron && concurrently -k \"vite\" \"pnpm dev:electron\"",
    "dev:electron": "wait-on tcp:3000 && electron .",
    "build": "cross-env BUILD_FOR_PROD=true pnpm scripts:build-electron && vite build",
    "pack:win": "pnpm build && electron-builder --windows",
    "pack:mac": "pnpm clean && pnpm build && electron-builder --macos && node .uploadoss.js",
    "pack4xintang": "pnpm clean && cross-env NODE_OPTIONS='--max-old-space-size=8192' cross-env channel=xintang node ./pack4prod.js",
    "pack4xintang-uat": "pnpm clean && cross-env NODE_OPTIONS='--max-old-space-size=8192' cross-env channel=xintang-uat node ./pack4prod.js",
    "clean": "tsx scripts/build/clean.ts",
    "type-check": "tsc",
    "jscpd": "jscpd ./ -o './report/'",
    "lint": "eslint --ignore-path .oms-lint-eslintignore . --ext .js,.ts,.jsx,.tsx --quiet && tsc",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006 --docs",
    "prettier": "prettier \"**/**/*.{js,jsx,ts,tsx,less}\" --write --log-level=error --cache",
    "test": "vitest run --coverage",
    "test-ui": "vitest --ui",
    "pre-commit": "pnpm type-check && lint-staged",
    "postinstall": "electron-builder install-app-deps",
    "scripts:build-electron": "tsx scripts/build/electron.ts",
    "scripts:gen-services-algo-types": "pnpm -C packages-mono/services gen-algo-types",
    "scripts:gen-services-types": "pnpm -C packages-mono/services gen-types",
    "scripts:gen-services-proto-json": "pnpm -C packages-mono/services gen-proto",
    "scripts:gen-services-access-code": "pnpm -C packages-mono/services gen-access-code",
    "scripts:gen-odm-column-fields": "pnpm -C apps/odm-fe gen-column",
    "dtm-dev": "pnpm -C apps/dtm-fe dev",
    "dtm-dev:test": "pnpm -C apps/dtm-fe dev:test",
    "dtm-build": "pnpm -C apps/dtm-fe build",
    "dtm-preview": "pnpm -C apps/dtm-fe preview",
    "dtm-lint": "pnpm -C apps/dtm-fe lint",
    "dtm-lint:fix": "pnpm -C apps/dtm-fe lint:fix",
    "odm-dev": "pnpm -C apps/odm-fe dev",
    "odm-lint": "pnpm -C apps/odm-fe lint",
    "odm-build": "pnpm -C apps/odm-fe build"
  },
  "husky": {
    "hooks": {
      "prepare": "husky install",
      "commit-msg": "npx commitlint -e $GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./.cz-changelog/index.js"
    },
    "cacheCleanupRequired": false
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "@babel/runtime",
        "require-from-string",
        "@testing-library/dom",
        "@storybook/core-common",
        "@storybook/node-logger",
        "webpack"
