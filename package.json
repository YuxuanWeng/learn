    "patchedDependencies": {
      "rc-picker@3.14.6": "patches/rc-picker@3.14.6.patch",
      "rc-picker@2.6.11": "patches/rc-picker@2.6.11.patch",
      "antd@4.21.3": "patches/antd@4.21.3.patch"
