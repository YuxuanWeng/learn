  "build": {
    "asar": true,
    "afterPack": "./after-pack.js",
    "asarUnpack": "resources/exes/**",
    "files": [
      "main",
      "src/out",
      "resources/**"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": {
      "provider": "generic",
      "url": "http://bdm-fe-zp.oss-cn-shanghai.aliyuncs.com/dist"
    },
    "productName": "OMS",
    "artifactName": "${productName}-${version}.${ext}",
    "appId": "OMS",
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.ico"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "signingHashAlgorithms": [
        "sha1",
        "sha256"
      ],
      "icon": "icon.ico",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "resources/build-icon.ico"
    }
  },
  "lint-staged": {
    "*{.js,.jsx,.ts,.tsx}": "eslint --ext .js,.ts,.jsx,.tsx --quiet --fix"
  },
  "jscpd": {
    "threshold": 4,
    "reporters": [
      "html",
      "console"
    ],
    "ignore": [
      "node_modules",
      "__tests__",
      ".cz-changelog",
      ".husky",
      ".storybook",
      ".vscode",
      "dist",
      "main",
      "mock",
      "public",
      "report",
      "resources",
      "src/out",
      ".coverage",
      "storybook-static",
      "**/*.stories.tsx",
      "**/*.spec.tsx"
    ],
    "absolute": true,
    "gitignore": true
  },
  "msw": {
    "workerDirectory": "public"
  }
}
